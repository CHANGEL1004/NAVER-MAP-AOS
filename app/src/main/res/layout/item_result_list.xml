<layout>
    <data>
        <import type="android.view.View"/>
        <variable
            name="viewModel"
            type="org.sopt.navermap.presentation.main.MainActivityViewModel" />
    </data>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/cl_main_search_result_linear">
    <View
        android:layout_width="0dp"
        android:layout_height="94dp"
        android:background="@drawable/ic_main_search_list_rectangle"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>
    <ImageView
        android:id="@+id/iv_main_search_location"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginStart="18dp"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        android:src="@drawable/ic_location_24"
        />
    <TextView
        android:id="@+id/tv_main_search_list_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="알고리즘"
        android:textColor="@color/blue"
        android:textAppearance="@style/Typography.NaverMap.Body1"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toEndOf="@id/iv_main_search_location"
        android:layout_marginStart="12dp"
        />
    <TextView
        android:id="@+id/tv_main_search_address"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="서울시 강남구"
        android:textColor="@color/gray_5"
        android:textAppearance="@style/Typography.NaverMap.Body7"
        app:layout_constraintTop_toBottomOf="@id/tv_main_search_list_title"
        android:layout_marginTop="6dp"
        app:layout_constraintStart_toEndOf="@id/iv_main_search_location"
        android:layout_marginStart="12dp"
        />
    <TextView
        android:id="@+id/tv_main_search_review"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="리뷰 12"
        android:textColor="@color/gray_5"
        android:textAppearance="@style/Typography.NaverMap.Body10"
        app:layout_constraintTop_toBottomOf="@id/tv_main_search_address"
        android:layout_marginTop="6dp"
        app:layout_constraintStart_toEndOf="@id/iv_main_search_location"
        android:layout_marginStart="12dp"
        />
    <TextView
        android:id="@+id/tv_main_search_category"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="퓨전음식"
        android:textColor="@color/gray_7"
        android:textAppearance="@style/Typography.NaverMap.Body9"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="21dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="18dp"
        />
    <TextView
        android:id="@+id/tv_main_search_distance"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="31km"
        android:textColor="@color/gray_7"
        android:textAppearance="@style/Typography.NaverMap.Body9"
        app:layout_constraintTop_toBottomOf="@id/tv_main_search_category"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="18dp"
        />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>